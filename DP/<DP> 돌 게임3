import sys
input = sys.stdin.readline

n = int(input()) #n입력
dp = [0] * (1001) 
dp[1] = 1
dp[3] = 1
dp[4] = 1

for i in range(5, n+1): #5부터 n까지 dp진행
    if not dp[i-1]: #dp[i-1]번째에 상대가 있으면
        dp[i] = 1 #이김
    if not dp[i-3]:
        dp[i] = 1
    if not dp[i-4]:
        dp[i] = 1

if dp[n]:
    print("SK")
else:
    print("CY")
    
#i-1, i-3, i-4번째에 상대가 있으면 SK가 무조건 이기게 된다.
